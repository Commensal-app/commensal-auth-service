{
    "extends": "airbnb",
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "global-require": "off",
        "no-underscore-dangle": "off",
        "class-methods-use-this": "off",
        "no-undef": "off",
        "consistent-return": "off"
    },
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "env": {
        "es6": true
    }
}